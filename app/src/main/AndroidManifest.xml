<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.forkmang">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-permission
        android:name="com.google.android.gms.permission.AD_ID"
        tools:node="remove" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:networkSecurityConfig="@xml/network_security_config"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ForkMangment"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
        <meta-data
            android:name="com.facebook.sdk.ClientToken"
            android:value="@string/facebook_client_token" />

        <activity
            android:name="com.forkmang.activity.PaymentScreenActivity"
            android:exported="false" />
        <!--
        <activity
            android:name="com.forkmang.fragment.LocationScreen_Fragment"
            android:exported="false" />
        -->
        <activity
            android:name="com.forkmang.activity.WalkinInQuee"
            android:exported="false" />
        <activity
            android:name="com.forkmang.activity.BookingSeat_ReserveConformationActivity"
            android:exported="false" />
        <activity
            android:name="com.forkmang.activity.Activity_PaymentSummary"
            android:exported="false" />
        <activity
            android:name="com.forkmang.activity.BookingTable_ReserveFood"
            android:exported="false" />
        <activity
            android:name="com.forkmang.activity.BookingOrder_ReserverConformationActivity"
            android:exported="false" />
        <activity
            android:name="com.forkmang.activity.Booking_TabView_Activity"
            android:exported="false" />
        <activity
            android:name="com.forkmang.activity.ScanOrderActivity"
            android:exported="false" />
        <activity
            android:name="com.forkmang.activity.DashBoardView"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.DashBoardActivity_2"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.DrawerActivity"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.forkmang.activity.TutorialScreen"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.SelectLanguage"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.RegisterActivity"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.RedirectToFacelogin"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.LoginFormActivity"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.LoginActivity"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.ForgotPassword"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.FaceLoginPermission"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.FaceLogin"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.BookingTable_DetailView"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.Activity_PaymentSummary_WalkinFragment"
            android:exported="true" />
        <activity
            android:name="com.forkmang.activity.Walkin_ConformationActivity"
