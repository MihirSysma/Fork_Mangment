<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:padding="@dimen/_1sdp"
    android:orientation="vertical">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_5sdp"
        android:layout_marginTop="@dimen/_5sdp"
        android:padding="@dimen/_2sdp">

        <TextView
            android:id="@+id/txt_queueno"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/generalsansmedium"
            android:gravity="center_vertical"
            android:text="Order Id:  4154214"
            android:textColor="@color/black"
            android:textSize="16sp" />

    </RelativeLayout>


    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:cardCornerRadius="7dp"
        app:cardElevation="7dp"
        app:cardMaxElevation="7dp"
        app:cardPreventCornerOverlap="true"
        app:cardUseCompatPadding="true">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:orientation="vertical">


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/layout_rectangle_gray">

                <TextView
                    android:id="@+id/txt_restroname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:fontFamily="@font/generalsansmedium"
                    android:text="StarBucks"
                    android:textColor="@color/black"
                    android:textSize="17sp" />


                <LinearLayout
                    android:id="@+id/linear_view_layout_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:layout_marginEnd="@dimen/_8sdp"
                    android:orientation="horizontal"
                    android:padding="@dimen/_2sdp"
                    android:visibility="visible">

                    <TextView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginStart="@dimen/_100sdp"
                        android:background="@drawable/black_rect"
                        android:padding="@dimen/_2sdp"
                        android:text="00"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="15sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/generalsansmedium"
                        android:padding="@dimen/_2sdp"
                        android:text=":"
                        android:textColor="@color/black"
                        android:textSize="15sp" />

                    <TextView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginStart="@dimen/_1sdp"
                        android:background="@drawable/black_rect"
                        android:padding="@dimen/_2sdp"
                        android:text="25"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="15sp" />

                    <ImageView
                        android:id="@+id/edit_rating_img"
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:src="@drawable/pencilsquare_green" />

                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:layout_marginStart="@dimen/_7sdp"
                        android:src="@drawable/eye_red" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linear_view_layout_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="@dimen/_9sdp"
                    android:layout_marginEnd="@dimen/_8sdp"
                    android:orientation="horizontal"
                    android:padding="@dimen/_1sdp"
                    android:visibility="gone">


                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/ic_baseline_check_24" />


                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linear_view_layout_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginTop="@dimen/_9sdp"
                    android:layout_marginEnd="@dimen/_8sdp"
                    android:orientation="horizontal"
                    android:padding="@dimen/_1sdp"
                    android:visibility="gone">


                    <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        android:scaleType="fitXY"
                        android:src="@drawable/delete" />


                </LinearLayout>


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_1sdp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txt_restrotime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:gravity="center_vertical"
                    android:text="Open Till 1.00 Pm"
                    android:textColor="@color/text_gray2"
                    android:textSize="13sp" />

                <TextView
                    android:id="@+id/txt_restrocontact"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:gravity="center_vertical"
                    android:text="Sar 99"
                    android:textColor="@color/black"
                    android:textSize="13sp" />


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_3sdp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txt_restrobranch"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:gravity="center_vertical"
                    android:text="Walkin 4 Tb, Pickup In 8 Minutes"
                    android:textColor="@color/text_gray2"
                    android:textSize="13sp" />

                <TextView
                    android:id="@+id/txt_payment_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:gravity="center_vertical"
                    android:text="7 Person"
                    android:textColor="@color/green"
                    android:textSize="13sp" />


            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_3sdp"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/txt_order_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_marginEnd="@dimen/_5sdp"
                    android:gravity="center_vertical"
                    android:text="7 Person"
                    android:textColor="@color/red"
                    android:textSize="13sp" />


            </RelativeLayout>

            <LinearLayout
                android:id="@+id/linear_starrating"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_7sdp"
                    android:padding="@dimen/_2sdp">


                    <TextView

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/generalsansmedium"
                        android:text="Food:"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                    <RatingBar
                        style="@style/CustomRatingBarStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/_14sdp"
                        android:layout_marginStart="@dimen/_70sdp"
                        android:rating="5" />


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_7sdp"
                    android:padding="@dimen/_2sdp">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/generalsansmedium"
                        android:text="Waiter:"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                    <RatingBar
                        style="@style/CustomRatingBarStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/_14sdp"
                        android:layout_marginStart="@dimen/_70sdp"
                        android:rating="5" />


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_7sdp"
                    android:padding="@dimen/_2sdp">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/generalsansmedium"
                        android:text="Experience:"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                    <RatingBar
                        style="@style/CustomRatingBarStyle"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/_14sdp"
                        android:layout_marginStart="@dimen/_70sdp"
                        android:rating="5" />


                </RelativeLayout>

            </LinearLayout>


        </LinearLayout>


    </androidx.cardview.widget.CardView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#EBEBEB"
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/_10sdp"/>


</LinearLayout>








